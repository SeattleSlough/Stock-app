<h1>Stock</h1>

<%= form_tag user_portfoliosstocks_path(user_id: session[:user_id]), :method => :post, :class => 'form-seach' do %>
 <div class="input-append">
     <%= collection_select :portfolio, :user_id, User.all, :id, :name %>
   <%= collection_select :portfoliosstock, :stock_id, Stock.all, :id, :symbol %>
   <button type="submit", method: 'post', class="btn">Add</button>
 </div>
<% end %>
